<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Daftar_Activity">

  <TextView
      android:id="@+id/tvLShubuh"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Shubuh"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.061"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.34" />

  <TextView
      android:id="@+id/tvLDzuhur"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Dzuhur"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.065"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvLShubuh"
      app:layout_constraintVertical_bias="0.119" />

  <TextView
      android:id="@+id/tvLAshar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Ashar"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.065"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvLDzuhur"
      app:layout_constraintVertical_bias="0.161" />

  <TextView
      android:id="@+id/tvLMagrib"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Magrib"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.065"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvLAshar"
      app:layout_constraintVertical_bias="0.183" />

  <TextView
      android:id="@+id/tvLIsya"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Isya"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.065"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvLMagrib"
      app:layout_constraintVertical_bias="0.25" />

  <TextView
      android:id="@+id/tvRShubuh"
      android:layout_width="120dp"
      android:layout_height="wrap_content"
      android:hint="04.26"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.694"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.34" />

  <TextView
      android:id="@+id/tvRDzuhur"
      android:layout_width="120dp"
      android:layout_height="wrap_content"
      android:hint="11.40"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.694"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvRShubuh"
      app:layout_constraintVertical_bias="0.118" />

  <TextView
      android:id="@+id/tvRAshar"
      android:layout_width="120dp"
      android:layout_height="wrap_content"
      android:hint="15.01"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.694"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvRDzuhur"
      app:layout_constraintVertical_bias="0.161" />

  <TextView
      android:id="@+id/tvRMagrib"
      android:layout_width="120dp"
      android:layout_height="wrap_content"
      android:hint="17.30"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.694"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvRAshar"
      app:layout_constraintVertical_bias="0.184" />

  <TextView
      android:id="@+id/tvRIsya"
      android:layout_width="120dp"
      android:layout_height="wrap_content"
      android:hint="18.45"
      android:textSize="20sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.694"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvRMagrib"
      app:layout_constraintVertical_bias="0.251" />

  <TextView
      android:id="@+id/tvLokasi"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Lokasi"
      android:textSize="18sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.062"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.04" />

  <EditText
      android:id="@+id/etLokasi"
      android:layout_width="260dp"
      android:layout_height="wrap_content"
      android:text="Yogyakarta"
      android:inputType="textPersonName"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.136"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/tvLokasi"
      app:layout_constraintVertical_bias="0.0" />

  <Button
      android:id="@+id/btnSearch"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="4dp"
      android:layout_marginRight="4dp"
      android:text="Search"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.242"
      app:layout_constraintStart_toEndOf="@+id/etLokasi"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.071" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="WaktuSholat"
      android:textSize="16sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.06"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/etLokasi"
      app:layout_constraintVertical_bias="0.049" />



  <TextView
      android:id="@+id/tvRLokasi"
      android:layout_width="250dp"
      android:layout_height="wrap_content"
      android:hint="Location"
      android:textSize="16sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.618"
      app:layout_constraintStart_toEndOf="@+id/textView3"
      app:layout_constraintVertical_bias="0.021" />

  </androidx.constraintlayout.widget.ConstraintLayout>